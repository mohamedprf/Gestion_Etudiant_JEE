    <table border=1 class="table table-hover">
        <thead>
            <tr>
                <th>User Id</th>
                <th>nom</th>
                <th>prenom</th>
                <th>sexe</th>
                <th>Email</th>
                <th>filiere</th>
                
                <th colspan=2>Action</th>
            </tr>
        </thead>
        <tbody>
            <c:forEach items="${etudiants}" var="etudiant">
                <tr>
                    <td><c:out value="${etudiant.id}" /></td>
                    <td><c:out value="${etudiant.nom}" /></td>
                    <td><c:out value="${etudiant.prenom}" /></td>
                    <td><c:out value="${etudiant.sexe}" /></td>
                    <td><c:out value="${etudiant.email}" /></td>
                    <td><c:out value="${etudiant.filiere}" /></td>
                    
                    <td><a  class="btn btn-success" href="EtudiantController?action=edit&id=<c:out value="${etudiant.id}"/>">Update</a></td>
                    <td><a class="btn btn-danger" href="EtudiantController?action=delete&id=<c:out value="${etudiant.id}"/>">Delete</a></td>
                </tr>
            </c:forEach>
        </tbody>
    </table>
    
    <p><a href="EtudiantController?action=insert"  class="btn btn-primary">Add Etudiant</a></p>

  
